{
  "name": "dj-larian",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "bash scripts/ensure-sqlite-schema.sh && NODE_OPTIONS='--import tsx' next dev",
    "dev:auto": "bash scripts/start-dev-with-auto-restart.sh",
    "build": "bash scripts/ensure-postgresql-schema.sh && node scripts/fix-prisma-types.mjs && NODE_OPTIONS='--import tsx' next build",
    "postinstall": "node scripts/fix-prisma-types.mjs || true",
    "start": "NODE_OPTIONS='--import tsx' next start",
    "lint": "eslint --config eslint.config.mjs \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --config eslint.config.mjs \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "prepare": "husky install || true",
    "db:setup:local": "bash scripts/setup-local-db.sh",
    "db:local": "bash scripts/setup-local-db.sh",
    "db:production": "bash scripts/setup-production-db.sh",
    "db:setup:production-url": "bash scripts/setup-database-url-production.sh",
    "db:reset:local": "rm -f prisma/dev.db && npm run db:setup:local",
    "db:import:backup": "node scripts/import-backup-to-sqlite.mjs",
    "db:fix:imageids": "node scripts/fix-imageids.mjs",
    "db:studio": "npx prisma studio",
    "db:migrate:production": "node scripts/migrate-db-production.mjs",
    "db:migrate:blob-images": "node scripts/migrate-blob-images-production.mjs",
    "db:migrate:all": "npm run db:migrate:production && npm run db:migrate:blob-images",
    "generate:waveform-video": "node scripts/generate-waveform-video.mjs"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-better-sqlite3": "^7.0.0",
    "@prisma/adapter-neon": "^7.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@sentry/nextjs": "^10.26.0",
    "@sparticuz/chromium-min": "^131.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^0.25.1",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "gsap": "^3.12.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.476.0",
    "next": "^16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "puppeteer": "^24.31.0",
    "puppeteer-core": "^24.31.0",
    "react": "latest",
    "react-day-picker": "^8.10.1",
    "react-dom": "latest",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.7",
    "recharts": "^2.15.2",
    "sharp": "^0.34.5",
    "sonner": "^2.0.3",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "zod": "^3.24.3",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "^5.2.1",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "^7.0.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.4",
    "typescript-eslint": "^8.47.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash scripts/lint-staged-eslint.sh",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
